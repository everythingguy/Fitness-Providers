{
  "name": "fitness-providers",
  "version": "1.0.0",
  "description": "This is a full scale website for students to find fitness instructors.",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "tailwindcss": "^2.2.7",
    "web-vitals": "^1.1.2",
    "dotenv": "^16.0.0",
    "webpack": "^4.44.2"
  },
  "scripts": {
    "build-scss": "sass scss/style.scss:src/assets/style.css",
    "build-css": "postcss tailwind-src/tailwind.css -o src/assets/main.css && npm run build-scss",
    "start": "react-scripts start",
    "silent-start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "e2e": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.duraken.com/fitness-providers/fitness-providers.git"
  },
  "author": "everythingguy",
  "license": "ISC",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/react": "^5.9.4",
    "@cypress/webpack-dev-server": "^1.4.0",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "cross-env": "^7.0.3",
    "cypress": "^8.3.0",
    "fitness-providers": "file:..",
    "sass": "^1.37.5"
  }
}