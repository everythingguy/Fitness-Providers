{
  "name": "fitness-providers",
  "version": "1.0.0",
  "description": "This is a full scale website for students to find fitness instructors.",
  "main": "app.js",
  "scripts": {
    "lint": "eslint -c .eslintrc.json .",
    "lint-fix": "eslint --fix .",
    "build": "copyfiles -u 2 'src/views/**/*.ejs' 'dist/views' && copyfiles -u 2 'src/Email/**/*.ejs' 'dist/Email' && tsc",
    "start": "node dist/app.js",
    "client": "npm run dev --prefix client",
    "silent-client": "npm run silent-start --prefix client",
    "server": "nodemon --config nodemon.json src/app.ts",
    "server:debug": "nodemon --config nodemon.json --inspect-brk src/app.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest",
    "coverage": "jest --collectCoverage"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.duraken.com/fitness-providers/fitness-providers.git"
