{
  "restartable": "rs",
  "ignore": [".git", "node_modules/", "dist/", "coverage/", "client/", "public/", "private/", "src/tests", "src/**/__tests__"],
  "watch": ["src/"],
  "execMap": {
    "ts": "node -r dotenv/config -r ts-node/register"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json,ts",
  "legacyWatch": true
}